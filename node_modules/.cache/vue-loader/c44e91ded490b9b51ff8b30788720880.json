">\n                <ion-icon :icon=\"informationCircle\" size=\"large\"></ion-icon>\n        </ion-thumbnail>\n            <ion-label>\n                    {{dato.nombre_estacion}}\n            </ion-label>\n    </ion-item>\n</template>\n\n<script lang=\"ts\">\nimport { \n    IonLabel,\n    IonThumbnail,\n    IonItem,\n    IonIcon,\n    modalController\n} from '@ionic/vue';\n\nimport { informationCircle } from 'ionicons/icons';\nimport { defineComponent } from 'vue';\nimport axios from 'axios';\nimport Modal from './Modal.vue';\n\nconst sitioURL  =  'https://api.sebastian.cl/grupo-c/climas';\nconst token = '2|viCVCaSYgP9hN2zk6UEhWYeLq0SmvArCcxoCW1T8';\n\nexport default defineComponent({\n    name: \"Home\",\n    setup() {\n        return { informationCircle };\n    },\n    components: {\n        IonItem,\n        IonThumbnail,\n        IonIcon,\n        IonLabel\n    },\n    data() {\n        return {\n            datos:null\n        }\n    },\n    mounted() {\n        this.getDatos();\n    },\n    methods: {\n        getDatos() {\n            axios.get(sitioURL, {\n                headers: {\n                    Authorization: 'Bearer ' + token,\n                    'Content-Type' : 'application/json',\n                    'Accept' : 'application/json'\n                }\n            })\n            .then(response => {\n                this.datos = response.data\n            })\n        },\n        \n        async openModal(nombreEstacion: string) {\n            const modal = await modalController\n                .create({\n                    component: Modal,\n                    cssClass: 'my-custom-class',\n                    componentProps: {\n                        titulo: nombreEstacion,\n                        mapa: nombreEstacion.replace(/[.', ]/g, '')\n                    },\n                })\n            return modal.present();\n        },\n    }\n});\n</script>\n\n<style scoped>\n    .vertical-center{\n        display: flex;\n        align-items: