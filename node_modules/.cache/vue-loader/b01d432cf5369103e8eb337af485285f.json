ts/CallAPI.vue","sourceRoot":"","sourcesContent":["<template>\n    <ion-page>\n        <ion-list>\n            <ion-item-sliding v-for=\"dato in datos\" :key=\"dato.id\">\n                <ion-item>\n                    <p>\n                        {{dato.nombre_estacion}}\n                    </p>\n                </ion-item>\n            </ion-item-sliding>\n        </ion-list>\n    </ion-page>\n</template>\n\n<script>\nimport { \n    IonItem, \n    IonList,\n    IonItemSliding\n} from '@ionic/vue';\n\nimport { defineComponent } from 'vue';\nimport axios from 'axios';\n\nconst sitioURL  =  'https://api.sebastian.cl/grupo-c/climas';\nconst token = '2|viCVCaSYgP9hN2zk6UEhWYeLq0SmvArCcxoCW1T8';\n\nexport default defineComponent({\n    components: {\n        IonItem,\n        IonList,\n        IonItemSliding\n    },\n    data() {\n        return {\n            datos:null\n        }\n    },\n    mounted() {\n        this.getDatos();\n    },\n    methods: {\n        getDatos() {\n            axios.get(sitioURL, {\n                headers: {\n                    Authorization: 'Bearer ' + token,\n                    'Content-Type' : 'application/json',\n                    'Accept' : 'application/json'\n                }\n            })\n            .then(response => {\n                this.datos = response.data\n            })\n        },\n    }\n});\n</script>\n\n<style scoped>\n    .centrado {\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        transform: translate(-50%, -50%);\n        color: black;\n    }\n\n    .centrado > strong {\n        text-shadow: 2px 2px 0px rgba(0,0,0,0.6);\n        color: white;\n    }\n</style>"]}]}