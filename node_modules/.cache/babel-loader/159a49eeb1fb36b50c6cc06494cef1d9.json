ion-item-sliding>\n        </ion-list>\n    </ion-page>\n</template>\n\n<script>\nimport { \n    IonItem, \n    IonList,\n    IonItemSliding,\n    IonItemOption, \n    IonItemOptions,\n    IonIcon\n} from '@ionic/vue';\n\nimport { informationCircle } from 'ionicons/icons';\n\nimport { defineComponent } from 'vue';\nimport axios from 'axios';\n\nconst sitioURL  =  'http://localhost:81/APIparalela/public/grupo-u/climas';\nconst token = '3|GdQhgz4VFO3JkEoqRufDGKFWolCKS9btpOjChMvA';\n\nexport default defineComponent({\n    components: {\n        IonItem,\n        IonList,\n        IonItemSliding,\n        IonItemOption, \n        IonItemOptions,\n        IonIcon,\n    },\n    setup() {\n        return {\n            informationCircle\n        }\n    },\n    data() {\n        return {\n            datos:null\n        }\n    },\n    mounted() {\n        this.getDatos();\n    },\n    methods: {\n        getDatos() {\n            axios.get(sitioURL, {\n                headers: {\n                    Authorization: 'Bearer ' + token,\n                    'Content-Type' : 'application/json',\n                    'Accept' : 'application/json'\n                }\n            })\n            .then(response => {\n                this.datos = response.data\n            })\n        },\n    }\n});\n</script>\n\n<style scoped>\n    ion-thumbnail {\n        position: relative;\n        display: inline-block;\n        text-align: center;\n    }\n\n    .centrado {\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        transform: translate(-50%, -50%);\n        color: black;\n    }\n\n    .centrado > strong {\n        text-shadow: 2px 2px 0px rgba(0,0,0,0.6);\n        color: white;\n    }\n</style>"],"sourceRoot":""}]}