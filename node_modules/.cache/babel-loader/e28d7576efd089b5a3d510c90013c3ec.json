ModalHome };\n    },\n    components: {\n        IonItem,\n        IonList,\n        IonItemSliding,/* \n        IonIcon, */\n        IonModal,\n        BaseModal\n    },\n    data() {\n        return {\n            datos:null\n        }\n    },\n    mounted() {\n        this.getDatos();\n    },\n    methods: {\n        getDatos() {\n            axios.get(sitioURL, {\n                headers: {\n                    Authorization: 'Bearer ' + token,\n                    'Content-Type' : 'application/json',\n                    'Accept' : 'application/json'\n                }\n            })\n            .then(response => {\n                this.datos = response.data\n            })\n        },\n    }\n});\n</script>\n\n<style scoped>\n    .centrado {\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        transform: translate(-50%, -50%);\n        color: black;\n    }\n\n    .centrado > strong {\n        text-shadow: 2px 2px 0px rgba(0,0,0,0.6);\n        color: white;\n    }\n</style>"],"sourceRoot":""}]}